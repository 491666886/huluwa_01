/* -----------------视频提示------------ */
.hint{
  height: 100%;
  width: 100%;
  z-index: 10;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
}

.hintHead{
  width: 100%;
  height: 100%;
  overflow: hidden;
  z-index: 2;
  background-color: black;
  opacity: 0.5
}
.hinContent image{
  width: 100%;
  height: 100%;
  position:absolute;
  background-size:100% 100%;
  z-index: -1;
}

.hinContent{
  width: 78%;
  height: 330rpx;
  position: absolute;
  top: 30%;
  left: 11%;
  z-index: 3;
}
.hintText{
  width: 100%;
  height: 120rpx;
  text-align: center;
  margin-top: 20%;
  margin-bottom: 10rpx;
  border-top-width: 0rpx;
  border-right-width: 0rpx;
  border-left-width: 0rpx;
  border-bottom-width: 1rpx;
  border-style: solid;
  border-color: gainsboro
}

.hintTextD{
  width: 100%;
  height: 120rpx;
