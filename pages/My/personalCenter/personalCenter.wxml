        <view class="user_child" wx-if='{{objectArray.length > 0}}'>
          
          <picker bindchange="bindPickerChange" value="{{index}}" range="{{objectArray}}" range-key="{{'childName'}}">
            <text>{{objectArray[index].childName}}</text>
              <!-- <text>{{objectArray[0].childName}}</text> -->
            <image src="/images/icon_x.png" class="bottomImg"></image>
          </picker>
          <!-- <view class="breckIm">
