page{
  background-color: #f7f8fa;
}
.list{
  border-top: 1px solid #E5E5E5;
  border-bottom: 1px solid #E5E5E5;
  padding: 0 30rpx;
  margin: 30rpx 0;
  background-color: #fff;
}
.item{
  height: 90rpx;
  border-bottom: 1px solid #E5E5E5;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.item:last-child{
  border-bottom: none;
}
.user_img{
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
}
.more{
  margin-left: 20rpx;
  width: 14rpx;
  height: 24rpx;
}
.item_r{
  display: flex;
  align-items: center;
}
.item_l{
  color: #222222;
  font-size: 32rpx;
}
.item_r text{
  color: #666666;
  font-size: 30rpx;
}

.wai{
  height: 100%;
  width: 100%;
}

/* --------------------剪切视频的  css----------------------------- */

page {
  min-height: auto;
  height: auto;
}

.cropper-wrapper {
  background: #fff;
}

.getCropperImage {
  text-align: center;
  font-size: 32rpx;
  display: flex;
  justify-content: space-around;
  position: absolute;
  bottom: 0;
  width: 750rpx;
  padding: 40 0rpx;
  background: #000;
  z-index: 9999;
  color: #fff;
  align-items: center;
}

.getCropperImage > view {
  padding: 0 40rpx;
  height: 60px;
  line-height: 60px;
}

.cropper-wrapper-bg {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgb(0, 0, 0);
  z-index: 22;
}

.rotate {
  width: 44rpx;
  height: 44rpx;
}