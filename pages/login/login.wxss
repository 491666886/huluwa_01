button{
  padding: 0;
  line-height: 90rpx
}
.phone{
    margin-bottom: 36rpx;
}
page{
  position: relative;
  height: 100%;
}
.bj{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.bj image:nth-child(1){
  width: 235rpx;
  height: 269rpx;
  margin-bottom: 15rpx;
  margin-top: 130rpx;
}
.bj image:nth-child(2){
  width: 320rpx;
  height: 35rpx;
  margin-bottom: 130rpx;
}
.cot{
  padding: 0 30rpx;
}
.cot button{
  color: #FFFFFF;
  height: 90rpx;
  width: 100%;
  
  margin-bottom: 36rpx;
  background:linear-gradient(0deg,rgba(254,131,56,1),rgba(255,169,48,1));
}
.cot view{
  text-align: center;
  color: #666666;
}
.footer{
  position: absolute;
  bottom: 100rpx;
  left: 0;
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 22rpx;
  justify-content: center;
}
.footer text{
  color: #FE8737;
}